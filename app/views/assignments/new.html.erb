<div class="row">
  <div class="col-sm-9 col-sm-offset-2">
    <h2>New assignment</h2>
  </div>
</div>
<%= form_for @assignment, html: {:class => "form-horizontal", :role => "form"},url: {action: "create"} do |f| %>

  <%= render partial: "form", :locals => {:f => f, :assignment => @assignment}%>

  <div class="row">
    <div class="col-sm-2 col-sm-offset-2 col-xs-4">
      <%= f.submit("Create assignment", :class => "btn btn-default") %>
    </div>
  </div>
<% end %>